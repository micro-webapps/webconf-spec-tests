{
    "version": "dev",
    "virtualhost": "localhost",
    "directories": {
        "./my-static-dir": {
            "alias": "/blog",
            "index": "index.php"
        }
    },
    "locations": {
        "/blog": {
            "proxy": {
                "protocol": "fcgi://",
                "hostname": "localhost",
                "port": 8081,
                "backend_alias": "/usr/share/wordpress/$1"
            }
        }
    }
}
